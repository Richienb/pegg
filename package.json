{
    "name": "pegg",
    "version": "3.0.2",
    "description": "FFMpeg with auto installation and a fluent api.",
    "repository": "https://github.com/Richienb/pegg.git",
    "author": "Richie Bendall <richiebendall@gmail.com>",
    "license": "MIT",
    "main": "index.js",
    "files": [
        "index.js",
        "index.d.ts",
        "utils/**/*",
        "lib/**/*",
        "scripts/**/*"
    ],
    "engines": {
        "node": ">=8.3"
    },
    "scripts": {
        "lint": "xo",
        "test": "yarn lint && ava",
        "postinstall": "node scripts/install.js"
    },
    "keywords": [
        "ffmpeg",
        "ffprobe",
        "ffbinaries",
        "fluent-ffmpeg",
        "automatic",
        "command"
    ],
    "dependencies": {
        "@octokit/rest": "^16.34.1",
        "appdata-path": "^1.0.0",
        "clean-semver": "^1.0.0",
        "conf": "^6.2.0",
        "download": "^7.1.0",
        "ergent": "^1.0.1",
        "execa": "^3.4.0",
        "ffbinaries-extra": "^1.1.1",
        "fluent-ffmpeg": "^2.1.2",
        "globby": "^10.0.1",
        "is-later": "^1.0.0",
        "is-windows": "^1.0.2"
    },
    "devDependencies": {
        "ava": "^3.3.0",
        "eslint-config-richienb": "^0.2.3",
        "fs-extra": "^8.1.0",
        "xo": "^0.25.3"
    },
    "resolutions": {
        "eslint": "^6.7.2"
    },
    "xo": {
        "extends": "richienb/node"
    }
}
